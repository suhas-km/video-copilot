{
  "name": "video-copilot",
  "version": "1.0.0",
  "description": "AI-powered video analysis and optimization platform",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:ci": "next lint --max-warnings=999999",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint:ci && npm run typecheck && npm run build",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "video",
    "analysis",
    "ai",
    "retention",
    "nextjs",
    "gemini",
    "deepgram"
  ],
  "author": "suhas-km <https://github.com/suhas-km>",
  "license": "Apache-2.0",
  "dependencies": {
    "@deepgram/sdk": "^4.11.3",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^1.4.1",
    "@google/generative-ai": "^0.24.1",
    "@tanstack/react-query": "^5.17.0",
    "@tensorflow/tfjs": "^4.22.0",
    "better-sqlite3": "^12.6.2",
    "canvas": "^2.11.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "fluent-ffmpeg": "^2.1.2",
    "framer-motion": "^12.29.0",
    "googleapis": "^170.1.0",
    "lucide-react": "^0.316.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^3.4.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "wavesurfer.js": "^7.7.0",
    "winston": "^3.11.0",
    "youtube-dl-exec": "^3.0.29",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.25.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.6",
    "@faker-js/faker": "^10.2.0",
    "@playwright/test": "^1.58.0",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.17",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
